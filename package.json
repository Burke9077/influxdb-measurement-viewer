{
  "name": "influx-stat-viewer",
  "version": "0.0.1",
  "description": "Stats viewer for InfluxDB",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serverreload": "/bin/bash -c '(/bin/bash docker-dev-startup.sh && echo \"waiting for influx\" && sleep 5 && nodemon app.js) || docker rm -f influxviewerdev'",
    "browsersync": "browser-sync start --proxy 'localhost:3000' --port 5000 --files 'public/**/*.*, views/**/*.*'",
    "dev": "npm-run-all -p serverreload browsersync"
  },
  "author": "burke9077@gmail.com",
  "license": "UNLICENSED",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3",
    "nodemon": "^3.0.1"
  }
}
